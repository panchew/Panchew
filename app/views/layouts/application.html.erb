<!DOCTYPE html>
<html>
<head>
  <title>Weallwin</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>

<div id="container">
	<div id="user_nav">
		<% if user_signed_in? %>
		  Signed in as <%= current_user.email %>. Not you?
		  <%= link_to "Sign out" %>
		<% else %>
		  <%= link_to "Sign up" %> or <%= link_to "Sing in" %>
		<% end %>
  </div>

  <%= yield %>

</div>

</body>
</html>
